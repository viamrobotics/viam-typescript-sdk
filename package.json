{
  "name": "@viamrobotics/sdk",
  "version": "0.0.41",
  "description": "",
  "main": "./dist/main.umd.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/robot.*",
    "dist/robot/**/*.d.ts",
    "dist/events.*",
    "dist/components/**/*.d.ts",
    "dist/extra/**/*.d.ts",
    "dist/main.*",
    "dist/gen/**/*.esm.d.ts",
    "dist/gen/**/*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.es.js",
      "require": "./dist/main.umd.js"
    }
  },
  "scripts": {
    "build": "vite build && npm run copy-dts",
    "check": "npm-check --production",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "doc": "typedoc",
    "doc:watch": "typedoc --watch",
    "format": "prettier --write './src/**/*.{ts,js}'",
    "lint:prettier": "prettier --check './src/**/*.{ts,js}'",
    "lint:eslint": "eslint './src/**/*.{ts,js}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viamrobotics/viam-typescript-sdk.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/viamrobotics/viam-typescript-sdk/issues"
  },
  "homepage": "https://github.com/viamrobotics/viam-typescript-sdk#readme",
  "dependencies": {
    "@viamrobotics/rpc": "^0.1.34"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.15.0-1",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@improbable-eng/grpc-web-fake-transport": "^0.15.0",
    "@types/google-protobuf": "^3.15.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "happy-dom": "^8.2.6",
    "npm-check": "^6.0.1",
    "prettier": "^2.8.1",
    "prettier-plugin-jsdoc": "^0.4.2",
    "ts-protoc-gen": "^0.15.0",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.28.4"
  }
}
